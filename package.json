{
  "name": "bl-connector",
  "version": "0.1.4-alpha",
  "description": "A template for creating Black Lion Connectors",
  "main": "BlConnector.js",
  "scripts": {
    "build": "webpack --progress --colors --watch",
    "build-dev": "NODE_ENV=development&& webpack --progress --colors --watch",
    "build-prod": "NODE_ENV=production&& webpack --progress --colors -p",
    "build-test": "NODE_ENV=test&& webpack --progress --colors --watch",
    "build-dev-win": "set NODE_ENV=development&& webpack --progress --colors --watch",
    "build-prod-win": "set NODE_ENV=production&& webpack --progress --colors -p",
    "build-test-win": "set NODE_ENV=test&& webpack --progress --colors --watch",
    "start": "webpack-dev-server --progress --colors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arus-project/bl-connector-template.git"
  },
  "author": "Jon Horner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arus-project/bl-connector-template/issues"
  },
  "homepage": "https://github.com/arus-project/bl-connector-template",
  "dependencies": {
    "convict": "^0.6.1",
    "superagent": "^0.21.0"
  },
  "devDependencies": {
    "6to5": "^3.3.10",
    "6to5-core": "^3.3.10",
    "6to5-loader": "^3.0.0",
    "webpack": "^1.5.3",
    "webpack-dev-server": "^1.7.0"
  }
}
